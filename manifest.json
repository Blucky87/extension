{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "content_scripts": [
    {
      "matches": ["*://*.instructure.com/*"],
      "js": ["jq.min.js","borderify.js"]
    }
  ],
  "web_accessible_resources":[
    "inject.js"
  ]

}
